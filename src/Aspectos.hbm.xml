<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping>

	<class name="Aspecto" table="Aspectos">
	
		<id name="id" type="int">
			<column name="id" length="11"/>
			<generator class="increment" />
		</id>

		<property name="nombre" type="string">
			<column name="NombreAspecto" length="30" not-null="true" />
		</property>
		
		<property name="idpersonaje" type="int">
			<column name="idPersonaje" length="10" not-null="false" />
		</property>
		
		<many-to-one name="Personaje" column="id" not-null="true" />	
		
		
	</class>

</hibernate-mapping>
